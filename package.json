{
  "name": "personal-site",
  "version": "0.0.1",
  "description": "Alex Kersov personal site",
  "main": "gulp.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://AlexO29@bitbucket.org/AlexO29/personal-site.git"
  },
  "author": "Alex Kersov",
  "license": "ISC",
  "homepage": "https://bitbucket.org/AlexO29/personal-site#readme",
  "dependencies": {
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-compass": "^2.1.0",
    "gulp-file-include": "^1.0.0",
    "gulp-sass": "^3.1.0",
    "sassdoc": "^2.1.20"
  },
  "paths": {
    "base": "./",
    "sass": {
      "input": "./sass/",
      "inputName": "sass",
      "output": "./stylesheets/",
      "temp": "./.temp/sass/"
    }
  },
  "settings": {
    "sass": {
      "inputFiles": "./sass/*.scss",
      "tempFiles": "./.temp/sass/*.css"
    },
    "sassdoc": {
      "dest": "./sassdoc"
    },
    "bake": {
      "inputFiles": "./views/index.html"
    }
  },
  "devDependencies": {
    "gulp-watch": "^4.3.11"
  }
}
